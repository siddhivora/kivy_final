<TestResultScreen>:
    BoxLayout:
        orientation: 'vertical'
        canvas:
            Color:
                rgba:1,1,1,1
            Rectangle:
                size: self.size

        FloatLayout:
            canvas:
                Color:
                    rgba: 0,0,0,0.3
                Rectangle:
                    size: (800,7)
                    pos: (0,420)
                Rectangle:
                    size: (800,7)
                    pos: (0,45)

        FloatLayout:
            Label:
                markup: True
                text: "[color=#000000][font=OpenSans]Patient id:[/color][/font]"
                size_hint: (None,None)
                pos: (55,415)
            Label:
                markup: True
                text: "[color=#000000][font=OpenSans]Name:[/color][/font]"
                size_hint: (None,None)
                pos: (43,395)
            Label:
                id: p_id
                markup: True
                text: "[color=#000000][font=OpenSans][/color][/font]"
                size_hint: (None,None)
                pos: (120,415)
            Label:
                id: p_name
                markup: True
                text: "[color=#000000][font=OpenSans][/color][/font]"
                size_hint: (None,None)
                pos: (95,395)
            Label:
                markup: True
                text: "[color=#000000][font=OpenSans]Test Results[/color][/font]"
                size_hint: (None,None)
                size: (50,50)
                pos: (385,425)
                font_size: '23px'
            Button:
                size_hint: (None,None)
                size: (50,50)
                background_color: [255,255,255,0]
                pos: (7,430)
                on_press: root.manager.current = "Home_Screen"
                canvas:
                    Rectangle:
                        source: 'Resources/home_btn.png'
                        size: (50,50)
                        pos: self.pos

    FloatLayout:
        Button:
            background_color: [0,0,200,0.8]
            markup: True
            text: "[color=#ffffff][b]Generate Report[/b][/color]"
	        size_hint: (None,None)
	        height: 38
            width: 130
            pos: (40,70)
            on_press: root.mail_popup()
        Button:
            background_color: [0,0,200,0.8]
		    markup: True
		    text: "[color=#ffffff][b]Proceed For Diagnosis[/b][/color]"
		    size_hint: (None,None)
		    height: 38
            width: 180
            pos: (600,70)
        Button:
            id: pre_btn
            background_color: [0,0,200,0.8]
            markup: True
            text: "[color=#ffffff][b]   Pre\nResults[/b][/color]"
	        size_hint: (None,None)
	        height: 50
            width: 70
            pos: (40,350)
            on_press: sm1.current= 'Pre_Result'
        Button:
            background_color: [0,0,200,0.8]
            markup: True
            text: "[color=#ffffff][b]  Post\nResults[/b][/color]"
	        size_hint: (None,None)
	        height: 50
            width: 70
            pos: (40,290)
            on_press: sm1.current= 'Post_Result'

    FloatLayout:
        Button:
            id: exit_btn
            size_hint: (None,None)
            height: 20
            width: 20
            background_color: [1,1,1,0]
            on_press : root.exit_prog()
            pos: (763,450)
            canvas:
                Rectangle:
                    source:"Resources/exit_btn.png"
                    pos: self.pos
                    size: self.size
        Button:
            id: back_btn
            size_hint: (None,None)
            height: 40
            width: 40
            background_color: [255,255,255,0]
            pos: (10,4)
            on_press: root.manager.current = 'Registration_Screen'
            canvas:
                Rectangle:
                    source:"Resources/back_btn.png"
                    pos: self.pos
                    size: self.size
        Label:
            markup: True
            id: contact_us
            text: "[font=OpenSans][color=#000000][u][ref=contact]Contact Us[/ref][/u][/color][/font]"
            underlined : True
            on_ref_press : root.manager.current = 'Contact_Screen'
            size_hint: (None,None)
            size: (50,50)
            pos: (365,-5)

    BoxLayout:
        ScreenManager:
            id: sm1
            Pre_Result:
            Post_Result:

